# Sample minimal config file.  Copy this to ~/.offlineimaprc and edit to
# suit to get started fast.

[general]
accounts = Test

[Account Test]
localrepository = Main
remoterepository = Example

[Repository Main]
type = Maildir
localfolders = ~/Test

[Repository Example]
type = IMAP
remotehost = examplehost
remoteuser = jgoerzen
